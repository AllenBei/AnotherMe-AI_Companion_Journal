{
  "functions": {
    "app/api/talk/journal/route.ts": {
      "maxDuration": 60
    },
    "app/api/**/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
} 